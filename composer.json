{
    "name": "chuckki/contao-iframe",
    "type": "contao-bundle",
    "description": "Skeleton for custom Contao 4 bundles",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "projektorientiert",
            "homepage": "https://www.projektorientiert.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "^4.4.1",
        "codefog/contao-haste":"^4.14.1"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Chuckki\\HvzIframeBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Chuckki\\HvzIframeBundle\\": "src/"
        }
    }
}
